{
    "extends": "airbnb-base",
    "rules":{
      "semi": 2,
      "import/no-extraneous-dependencies": [
          "error", {
              "devDependencies": true
          }]  
    },
    "globals":{
        "describe": true,
        "it": true,
        "expect": true,
        "app": true,
        "beforeEach": true,
        "request": true,
        "td": true,
        "joi": true,
        "joiAssert": true
    },
    "plugins": [
        "import"
    ]
}